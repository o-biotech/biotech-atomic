{
  "tasks": {
    "build": "deno task build:fmt && deno task build:lint && deno task test",
    "build:fmt": "deno fmt",
    "build:lint": "deno lint",
    "deploy": "deno fmt && deno task build && ftm git",
    "dev-config": "deno task dev-tailwind",
    "dev-tailwind": "echo '// deno-lint-ignore no-empty\n{}' > tailwind.config.js",
    "test": "deno test -A ./tests/tests.ts --coverage=cov"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "imports": {
    "$deepsignal": "https://esm.sh/deepsignal@1.3.4",
    "immer": "https://cdn.skypack.dev/immer@8.0.1?dts",
    "preact": "https://esm.sh/preact@10.16.0",
    "preact/": "https://esm.sh/preact@10.16.0/",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.1.5",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.3.1",
    "preact-render-to-string": "https://esm.sh/preact-render-to-string@6.2.0",
    "$std/": "https://deno.land/std@0.195.0/"
  },
  "lock": false,
  "fmt": {
    "files": {
      "include": [],
      "exclude": []
    },
    "options": {}
  },
  "lint": {
    "files": {
      "include": [],
      "exclude": []
    },
    "rules": {
      "include": [],
      "exclude": []
    }
  }
}
